---
import BaseLayout from "@/layouts/BaseLayout.astro";
import Contact from "@/components/forms/Contact.astro";
import Cta from "@components/blog/Cta.astro";
import { type AstroSeoProps } from "@astrolib/seo";

const seo: AstroSeoProps = {
	title: "Contact Majestico Studio: Get in Touch for Innovative Web Solutions",
	description:
		"Reach out to Majestico Studio for expert web design and SEO services. Connect with us to discuss your project and explore cutting-edge web solutions.",
	canonical: "https://majestico.co/it/contact/",
};
---

<BaseLayout seo={seo}>
	<Contact />
	<Cta class="col-span-full col-start-1 row-start-3" />
</BaseLayout>
